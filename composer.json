  "type": "library",
  "keywords": [
    "MySQL",
    "MySQL PHP",
    "PHP",
    "queries",
    "requests"
  ],
  "homepage": "https://github.com/AlexRoar/DAIM",
  "license": "MIT",
  "authors": [
    {
      "name": "Aleksandr Dremov",
      "email": "dremov.me@gmail.com",
      "role": "Developer"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "ext-mysqli": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "php": ">=7.1",
    "phpunit/phpunit": "^7"
  },
  "autoload": {
    "psr-4": {
      "DAIM\\": "src/DAIM"
    }
  },
  "autoload-dev": {
    "psr-4": {
