{
  "map": {
    "insert": {
      "into": {
        "{{table_name}}": {
          "values": {
            "{{values_group}}": "{{query_end}}"
          },
          "{{open_parenthesis}}": {
            "{{column_names}}": {
              "{{close_parenthesis}}": {
                "values": "../../../../values",
                "select": "/select"
              }
            }
          }
        }
      }
    },
    "update": {
      "{{table_name}}": {
        "set": {
          "{{column_value_groups}}": {
            "where": "select/*/from/{{table_name_group}}/where"
          }
        }
      }
    },
    "delete": {
      "from": {
        "{{table_name}}": {
          "where": "select/*/from/{{table_name_group}}/where",
          "{{query_end}}": "{{query_end}}"
        }
      }
    },
    "select": {
      "*": {
        "from": {
          "{{table_name_group}}": {
            "where": {
              "{{conditions}}": {
                "{{query_end}}": "{{query_end}}",
                "group by": {
                  "{{column_names}}": {
                    "having": {
                      "{{conditions}}": "../../"
                    },
                    "{{query_end}}": "{{query_end}}",
                    "order by": "../../../order by"
                  }
                },
                "order by": {
                  "{{sort_orders_group}}": "{{sort_orders_group}}"
                }
              }
            },
            "{{query_end}}": "{{query_end}}"
          }
        }
      },
      "{{column_names}}": {
        "from": "/select/*/from"
      }
    }
  },
  "logicOperators": {
    "largerThan": ">",
    "lessThan": "<",
    "largerOrEqualThan": ">=",
    "lessOrEqualThan": "<=",
    "notEqual": "!=",
    "equal": "=",
    "equalNull": "IS NULL",
    "notEqualNull": "IS NOT NULL",
    "between": "BETWEEN",
    "inList": "IN",
    "like": "LIKE",
    "exists": "EXISTS"
  },
  "compareToSet": {
    "all": "ALL"
  },
  "logicConjunctions": {
    "and": "AND",
    "or": "OR"
  },
  "functions": {
    "concatenate": "CONCAT([string])",
    "translate": "TRANSLATE(column, string, string, {FROM} table)",
    "replace": "REPLACE(column, string, string)",
    "upper": "UPPER(string)",
    "lower": "LOWER(string)",
    "substr": "SUBSTR(column, int, int)",
    "instr": "INSTR(column, string, int, int)",
    "ltrim": "LTRIM(column, string)",
    "rtrim": "RTRIM(column, string)",
    "decode": "DECODE(column, (string, string,[string]))",
    "length": "LENGTH(column)",
    "ifnull": "IFNULL(column, string)"
  }
}
